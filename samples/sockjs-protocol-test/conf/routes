# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

->        /echo                                      controllers.Application.Echo
->        /close                                     controllers.Application.Closed
->        /disabled_websocket_echo                   controllers.Application.EchoWithNoWebsocket
->        /cookie_needed_echo                        controllers.Application.EchoWithJSessionId

->        /actor/echo                                controllers.ApplicationActor.Echo
->        /actor/close                               controllers.ApplicationActor.Closed
->        /actor/disabled_websocket_echo             controllers.ApplicationActor.EchoWithNoWebsocket
->        /actor/cookie_needed_echo                  controllers.ApplicationActor.EchoWithJSessionId

->        /java/echo                                 controllers.j.Echo
->        /java/close                                controllers.j.Closed
->        /java/disabled_websocket_echo              controllers.j.EchoWithNoWebsocket
->        /java/cookie_needed_echo                   controllers.j.EchoWithJSessionId

->        /java_actor/echo                           controllers.actorj.Echo
->        /java_actor/close                          controllers.actorj.Closed
->        /java_actor/disabled_websocket_echo        controllers.actorj.EchoWithNoWebsocket
->        /java_actor/cookie_needed_echo             controllers.actorj.EchoWithJSessionId
