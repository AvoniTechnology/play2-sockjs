# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
->        /echo                                      controllers.Application.echo
->        /close                                     controllers.Application.closed
->        /disabled_websocket_echo                   controllers.Application.disabledWebSocketEcho
->        /cookie_needed_echo                        controllers.Application.cookieNeededEcho

->        /actor/echo                                controllers.ApplicationActor.echo
->        /actor/close                               controllers.ApplicationActor.closed
->        /actor/disabled_websocket_echo             controllers.ApplicationActor.disabledWebSocketEcho
->        /actor/cookie_needed_echo                  controllers.ApplicationActor.cookieNeededEcho

->        /java/echo                                 controllers.ApplicationJ.echo
->        /java/close                                controllers.ApplicationJ.closed
->        /java/disabled_websocket_echo              controllers.ApplicationJ.disabledWebSocketEcho
->        /java/cookie_needed_echo                   controllers.ApplicationJ.cookieNeededEcho

->        /java_actor/echo                           controllers.ApplicationActorJ.echo
->        /java_actor/close                          controllers.ApplicationActorJ.closed
->        /java_actor/disabled_websocket_echo        controllers.ApplicationActorJ.disabledWebSocketEcho
->        /java_actor/cookie_needed_echo             controllers.ApplicationActorJ.cookieNeededEcho

